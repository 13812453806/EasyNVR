<CONFIGURATION>
	<SERVER>
		<PREF NAME="connection_timeout" TYPE="UInt32" >0</PREF>
		<PREF NAME="error_logfile_name" >Error</PREF>
		<PREF NAME="error_logfile_dir" >c:\Program Files\EasyNVR\Logs\</PREF>
		<PREF NAME="error_logfile_interval" TYPE="UInt32" >7</PREF>
		<PREF NAME="error_logfile_size" TYPE="UInt32" >10240</PREF>
		<PREF NAME="error_logfile_verbosity" TYPE="UInt32" >2</PREF>
		<PREF NAME="screen_logging" TYPE="Bool16" >true</PREF>
		<PREF NAME="error_logging" TYPE="Bool16" >true</PREF>
		<PREF NAME="run_num_threads" TYPE="UInt32" >3</PREF>
		<PREF NAME="pid_file" >EasyDarwin.pid</PREF>
		<PREF NAME="force_logs_close_on_write" TYPE="Bool16" >false</PREF>
		<PREF NAME="run_num_blocking_threads" TYPE="UInt32" >1</PREF>
	</SERVER>
	<MODULE NAME="QTSSErrorLogModule" ></MODULE>
	<MODULE NAME="EasyNVRModule" >
		<PREF NAME="channels_prefs_file" >channels.xml</PREF>
	</MODULE>
	<MODULE NAME="EasyCMSModule" >
		<PREF NAME="easycms_ip" >127.0.0.1</PREF>
		<PREF NAME="easycms_port" TYPE="UInt16" >10001</PREF>
		<PREF NAME="device_serial" >001002000002</PREF>
		<PREF NAME="device_name" >NVR002</PREF>
		<PREF NAME="device_key" >123456</PREF>
		<PREF NAME="device_tag" >dev</PREF>
		<PREF NAME="keep_alive_interval" TYPE="UInt32" >30</PREF>
	</MODULE>
</CONFIGURATION>
